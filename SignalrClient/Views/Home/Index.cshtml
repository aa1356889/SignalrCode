    <script>
        
        function LoadMenu() {
            addTabs(({ id: '2', title: '我的控制台', close: false, url: '@Url.Action("Index","MyConsole")' }));
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetMenus")",
                success: function (ajadata) {
                    var menuDoms = [];
                    myCommon.Ajax.ResponseReulstProcess(ajadata, function() {
                        var parentMenu = ajadata.Data.filter(function (c) {
                            return c.ParentId == -1;
                        });
                        if (parentMenu.length <= 0) {
                            return;
                        }

                        parentMenu.forEach(function (c) {

                            menuDoms[menuDoms.length] = {
                                id: c.MenuId,
                                text: c.MenuName,
                                icon: c.Icon,
                                children: []
                            }
                            LoadMenuData(menuDoms[menuDoms.length-1], ajadata.Data);
                        });
                        $('.page-sidebar-menu').sidebarMenu({ data: menuDoms, param: { strUser: 'admin' } });

                        App.fixIframeCotent();
                    });
                }
            });
        }
        function LoadMenuData(Menu, Menus) {
         
            var sons = Menus.filter(function (c) {
                return c.ParentId == Menu.id;
            });
            if (sons.length == 0) {
                return;
            }
            sons.forEach(function(c) {
                var sonData = { id: c.MenuId, text: c.MenuName, url: (c.Area ? "/" + c.Area : "") + (c.Controller ? "/" + c.Controller : "") + (c.Action ? "/" + c.Action : ""), targetType: "iframe-tab", icon: c.Icon, children: [] }
                LoadMenuData(sonData, Menus);
                Menu.children[Menu.children.length] = sonData;
            });
        }
        var qs = { 'Uid': '@CurrentUser.UserId', UName: '@CurrentUser.UserName', "HeadPath": "/Pulg/LayuiForLayIM_2.0.3/timg.jpg", AuthorizationCode: '@passWord',OrganId:"@CurrentUser.OrganId",Depid:"@CurrentUser.DepartmentId" };
        $(function () {
            LoadMenu();
         
            var soket = new SignalrClient(qs);
            soket.Connection();


        });

    </script>
