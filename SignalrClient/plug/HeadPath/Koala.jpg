		$(function () {
			// 引入数据到独立JS文件
		    window.url = "/building";

		    var banner_item = $(".banner_item");
		    var html = [];

		    for (var jj = 0; jj < $("#banner_list li").length; jj++) {
		        if (jj == 0) {
		            html.push("<li class=\"active\"></li>");
		        } else {
		            html.push("<li></li>");
		        }
		    }
            
		    banner_item.append(html.join(""));
		    $("#banner_list li[url!='']").css("cursor", "pointer");
		    $("#banner_list li").click(function () {
		        if ($(this).attr("url") != "") {
		            window.open($(this).attr("url"));
		        }
		    });
		    //$('.mod-slider').modslider({ width: 640, height: 320, during: 3000 });
		});
